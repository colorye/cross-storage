{
  "name": "@colorye/cross-storage",
  "version": "1.3.1",
  "description": "Access localStorage from other domain you've owned.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist && yarn min",
    "min": "terser dist/index.js --compress --mangle --mangle-props regex=/^__/,reserved=[__esModule] --module --output dist/index.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cross-storage",
    "postMessage",
    "shared"
  ],
  "author": "ryenguyen7411 <ryenguyen7411@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/colorye/cross-storage",
  "repository": {
    "type": "git",
    "url": "https://github.com/colorye/cross-storage.git"
  },
  "dependencies": {
    "idb": "6.1.2",
    "js-cookie": "2.2.1",
    "nanoid": "2.1.7"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-transform-modules-commonjs": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.28.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "terser": "4.4.0"
  }
}
