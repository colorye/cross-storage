  "homepage": "https://github.com/colorye/cross-storage",
  "repository": {
    "type": "git",
    "url": "https://github.com/colorye/cross-storage.git"
  },
  "dependencies": {
    "js-cookie": "^2.2.1",
    "nanoid": "^2.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "terser": "^4.4.0"
  }
